<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NewFestivalPost" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorTheame</item>
        <item name="colorPrimaryVariant">@color/colorTheame</item>
        <item name="colorOnPrimary">@color/colorTheame</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/colorTheame</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorTheame</item>
        <item name="colorPrimaryDark">@color/colorTheame</item>
        <item name="colorAccent">@color/colorTheame</item>
        <item name="android:textColor">@color/colorTheame2</item>
    </style>
    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/colorTheame</item>
        <item name="colorPrimaryDark">@color/colorTheame</item>
        <item name="colorAccent">@color/colorTheame</item>
    </style>

    <style name="Spinner" parent="Widget.AppCompat.Light.DropDownItem.Spinner">

        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">@dimen/_19sdp</item>
    </style>
</resources>